.contanier, .header, .content {
  width: 100%;
}

.contanier {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.header {
  box-sizing: border-box;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 15rpx;
  border-top: 1px solid #f5f5f5;
}